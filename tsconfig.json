{
  "compileOnSave": false,
  "include": [
    "./ClientApp",
    "./wwwroot/ts"
  ],
  "compilerOptions": {
    "outDir": "./wwwroot/clientapp/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "importHelpers": true,
    "target": "ES5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "module": "es2015",
    "baseUrl": "./"
  }
}
